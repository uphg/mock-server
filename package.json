{
  "name": "mock-server-builder",
  "version": "0.1.0-alpha.5",
  "description": "Automatically generate mock data routing services based on configuration files",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "mock-server": "./bin/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node --test tests/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "docs:generate": "node src/generate-docs.js",
    "docs:generate:watch": "node --watch src/generate-docs.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "mock",
    "server",
    "api",
    "json"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-async": "^2.2.0",
    "markdown-it-container": "^4.0.0",
    "mdast-builder": "^1.1.1",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.2",
    "path-to-regexp": "^8.2.0"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "supertest": "^7.1.4",
    "vitepress": "^1.6.4"
  }
}